<div class="field row">
  <div class="col-md-3" >
    <%= f.attribute_fields do |a| %>
      <%= a.attribute_select %>
    <% end %>
  </div>
  <div class="col-md-3">
    <%= f.predicate_select %>
  </div>
  <div class="col-md-3">
    <%= f.value_fields do |v| %>
      <%= v.text_field :value %>
    <% end %>
  </div>
  <div class="col-md-3">
    <%= link_to t("admin.user.remove"), "#", class: "remove_fields" %>
  </div>
</div>
